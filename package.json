{
  "name": "project2",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.0",
    "npm": "6.13.7"
  },
  "description": "A Simple Login Example using Node.js, Mongo and Redis",
  "main": "./server/app.js",
  "scripts": {
    "buildLoginBundle": "babel ./dist/client/login ./dist/client/helper --presets @babel/preset-react --out-file ./hosted/loginBundle.js",
    "buildAppBundle": "babel ./dist/client/app ./dist/client/helper --presets @babel/preset-react --out-file ./hosted/bundle.js",
    "buildSignupBundle": "babel ./dist/client/signup ./dist/client/helper --presets @babel/preset-react --out-file ./hosted/signupBundle.js",
    "buildPortalBundle": "babel ./dist/client/portal ./dist/client/helper --presets @babel/preset-react --out-file ./hosted/portalBundle.js",
    "build": "npm run tsc && npm run buildLoginBundle && npm run buildAppBundle && npm run buildSignupBundle && npm run buildPortalBundle",
    "watchLoginBundle": "babel --watch ./dist/client/login ./dist/client/helper --presets @babel/preset-react --out-file ./hosted/loginBundle.js",
    "watchAppBundle": "babel --watch ./dist/client/app ./dist/client/helper --presets @babel/preset-react --out-file ./hosted/bundle.js",
    "watchSignupBundle": "babel --watch ./dist/client/signup ./dist/client/helper --presets @babel/preset-react --out-file ./hosted/signupBundle.js",
    "watchPortalBundle": "babel --watch ./dist/client/portal ./dist/client/helper --presets @babel/preset-react --out-file ./hosted/portalBundle.js",
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "start": "node ./server/app.js",
    "pretest": "eslint ./server --fix && npm run tslint",
    "test": "echo \"Tests complete\"",
    "cstart": "concurrently \"npm:nodemon\" \"npm:watchLoginBundle\" \"npm:watchAppBundle\" \"npm:watchSignupBundle\" \"npm:watchPortalBundle\" \"npm:watchTs\"",
    "tslint": "tslint --project tsconfig.json --fix",
    "watchTs": "tsc --watch",
    "build:ts": "tsc"
  },
  "keywords": [
    "Node",
    "MVC",
    "Redis",
    "Login"
  ],
  "author": "Nathan Glick",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.9.4",
    "@types/jquery": "^3.3.34",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.1.0",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.3",
    "mongoose": "^5.9.5",
    "redis": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.9.2"
  }
}
